    //     "serverId": "my_orga_in_sonarcloud.io",
    //     "serverUrl": "https://sonarcloud.io",
    //     "organizationKey": "my_organization",
    //     "token": "V2VkIE1..."
    //   }
    // ],
    // Focus on new code issues (optional)
    "sonarlint.focusOnNewCode": false,
    // Disable telemetry (recommended for privacy)
    "sonarlint.disableTelemetry": true,
    "Lingma.aI Chat.mcpToolsInAgentMode": true,
    "chat.mcp.discovery.enabled": {
        "claude-desktop": true,
        "windsurf": true,
        "cursor-global": true,
        "cursor-workspace": true
    },
    "chatgpt.openOnStartup": true,
    "chat.mcp.assisted.nuget.enabled": true,
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "githubIssues.alwaysPromptForNewIssueRepo": true,
