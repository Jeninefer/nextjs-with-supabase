{
    "git.autofetch": true,
    "git.autoStash": false,
    "git.confirmSync": true,
    "git.enableSmartCommit": false,
    "git.postCommitCommand": "",
    "git.showPushSuccessNotification": true,
    "git.untrackedChanges": "separate",
    "scm.defaultViewMode": "tree",
    "json.schemaDownload.enable": true,
    "typescript.disableAutomaticTypeAcquisition": true,
    "redhat.telemetry.enabled": false,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.suggest.autoImports": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "explicit"
    },
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.defaultProfile.osx": "bash",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "files.associations": {
        "*.tsx": "typescriptreact",
        "*.ts": "typescript",
        "*.json": "jsonc"
    },
    "emmet.includeLanguages": {
        "typescript": "html",
        "typescriptreact": "html"
    },
    "tailwindCSS.includeLanguages": {
        "typescript": "html",
        "typescriptreact": "html"
    },
    "tailwindCSS.experimental.classRegex": [
        "cn\\(([^)]*)\\)"
    ],
    "python.defaultInterpreterPath": "/usr/local/bin/python3",
    "python.terminal.activateEnvironment": false,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/.next": true,
        "**/__pycache__": true,
        "**/*.pyc": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/.next": true,
        "**/dist": true,
        "**/__pycache__": true
    },
    "eslint.workingDirectories": [
        {
            "directory": ".",
            "changeProcessCWD": true
        }
    ],
    "nextjs.serverPort": 3000,
    "cs-script.execMode": "Standard",
    "cs-script.syntaxCheckOnSave": true,
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "shellscript"
    ],
    "python.analysis.extraPaths": [
        "/Users/jenineferderas/.vscode/extensions/ms-security.ms-sentinel-1.0.0/python"
    ],
    "sonarlint.connectedMode.project": {
        "connectionId": "jeninefer",
        "projectKey": "Jeninefer_nextjs-with-supabase"
    },
    "editor.codeActions.triggerOnFocusChange": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    // Exclude generated or library files from SonarLint analysis
    "sonarlint.analysisExcludesStandalone": "**/lib/**,**/*generated*",
    // Enable on-the-fly analysis (default: true)
    "sonarlint.automaticAnalysis": true,
    // Example: connect to SonarQube/SonarCloud (add your connection info if needed)
    // "sonarlint.connectedMode.connections.sonarcloud": [
